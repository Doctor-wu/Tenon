{
  "el": "a-carousel",
  "props": {
    "style": "carouselStyle",
    "<BINDING>": "carouselConfig"
  },
  "children": [
    {
      "el": "a-carousel-item",
      "props":{
        "style": {
          "width": "75%",
          "border-radius": "8px",
          "display": "flex",
          "align-items": "stretch",
          "justify-content": "stretch"
        }
      },
      "children": [
        {
          "el": "Picture",
          "props": {
            "imageProps": {
              "src": "{{this.item}}",
              "preview": true
            },
            "imageStyle": {
              "width": "100%",
              "display": "flex",
              "height": "100%"
            }
          }
        }
      ],
      "<FOR>": "this.items"
    }
  ]
}
