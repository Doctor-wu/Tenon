{
  "name": "Radio",
  "icon": "file",
  "description": ["Radio组件"],
  "dependencies": [],
  "platform": [
    "web"
  ],
  "events": {
    "onChange": "值改变时"
  },
  "schemas": [
    {
      "type": "object",
      "title": "单选框属性",
      "fieldName": "radioConfig",
      "properties": {
        "model-value": {
          "type": "string",
          "title": "单选框绑定值"
        },
        "content": {
          "type": "string",
          "title": "单选框label",
          "default": "单选框"
        },
        "type": {
          "type": "select",
          "title": "单选框类型",
          "options": {
            "radio": "radio",
            "button": "button"
          },
          "default": "radio"
        },
        "value": {
          "type": "string",
          "title": "选项的值",
          "default": "value"
        },
        "disabled": {
          "type": "boolean",
          "title": "按钮是否禁用",
          "default": false
        }
      }
    }
  ],
  "tenonProps": {
    "containerStyle": {
      "box-sizing": "border-box"
    }
  }
}