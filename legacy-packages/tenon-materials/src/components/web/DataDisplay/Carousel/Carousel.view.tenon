<ArcoCarousel style="carouselStyle" t-bind="carouselConfig">
  <ArcoCarouselItem
    t-for="this.carouselConfig.pics"
    style="{
      width: '75%',
      borderRadius: '8px',
      display: 'flex',
      alignItems: 'stretch',
      justifyContent: 'stretch'
    }"
  >
    <Picture
      imageProps="{src: '{{this.item}}', preview: true}"
      imageStyle="{width: '100%', display: 'flex', height: '100%'}"
    ></Picture>
  </ArcoCarouselItem>
</ArcoCarousel>